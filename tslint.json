{
    "extends": ["tslint:latest", "tslint-react"],
    "rules": {
        "arrow-parens": false,
        "no-magic-numbers": true,
        "interface-name": false,
        "member-ordering": [true, {
          "order": [
            "static-field",
            "static-method",
            "constructor",
            "private-instance-method",
            "protected-instance-method",
            "public-instance-method"
          ]
        }],
        "jsx-no-multiline-js": false
    }
}
